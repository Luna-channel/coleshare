{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "env": {
    "DATABASE_URL": {
      "description": "Neon数据库连接字符串"
    },
    "BLOB_READ_WRITE_TOKEN": {
      "description": "Vercel Blob存储令牌"
    },
    "ADMIN_KEY": {
      "description": "管理员访问令牌"
    },
    "MEMBER_KEY": {
      "description": "会员访问令牌（可选）"
    }
  }
} 